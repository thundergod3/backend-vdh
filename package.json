{
  "name": "app-back-end",
  "version": "1.0.0",
  "description": "loop back end",
  "keywords": [
    "loopback-application",
    "loopback"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "engines": {
    "node": ">=10.16"
  },
  "scripts": {
    "build": "lb-tsc",
    "build:watch": "lb-tsc --watch",
    "lint": "npm run prettier:check",
    "lint:fix": "npm run prettier:fix",
    "prettier:cli": "lb-prettier \"**/*.ts\" \"**/*.js\"",
    "prettier:check": "npm run prettier:cli -- -l",
    "prettier:fix": "npm run prettier:cli -- --write",
    "pretest": "npm run clean && npm run build",
    "test": "lb-mocha --allow-console-logs \"dist/__tests__\"",
    "posttest": "npm run lint",
    "test:dev": "lb-mocha --allow-console-logs dist/__tests__/**/*.js && npm run posttest",
    "docker:build": "docker build -t app-back-end .",
    "docker:run": "docker run -p 3000:3000 -d app-back-end",
    "migrate": "node ./dist/migrate",
    "openapi-spec": "node ./dist/openapi-spec",
    "prestart": "npm run build",
    "start": "node -r source-map-support/register .",
    "clean": "lb-clean dist *.tsbuildinfo .eslintcache"
  },
  "repository": {
    "type": "git"
  },
  "author": "",
  "license": "",
  "files": [
    "README.md",
    "dist",
    "src",
    "!*/__tests__"
  ],
  "dependencies": {
    "@loopback/authentication": "^5.0.0",
    "@loopback/authentication-jwt": "^0.5.1",
    "@loopback/authentication-passport": "^2.1.13",
    "@loopback/authorization": "^0.6.6",
    "@loopback/boot": "^2.5.1",
    "@loopback/core": "^2.9.5",
    "@loopback/cron": "^0.2.13",
    "@loopback/mock-oauth2-provider": "^0.2.0",
    "@loopback/openapi-v3": "^3.4.9",
    "@loopback/repository": "^2.11.2",
    "@loopback/rest": "^6.2.0",
    "@loopback/rest-explorer": "^2.2.10",
    "@loopback/security": "^0.2.18",
    "@loopback/service-proxy": "^2.3.8",
    "@types/agenda": "^2.0.9",
    "@types/axios": "^0.14.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/es6-promise": "^3.3.0",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/loopback": "^3.2.9",
    "@types/moment": "^2.13.0",
    "@types/multer": "^1.4.4",
    "@types/nodemailer": "^6.4.0",
    "@types/passport": "^1.0.4",
    "@types/passport-facebook": "^2.1.10",
    "@types/passport-local": "^1.0.33",
    "@types/qs": "^6.9.5",
    "@types/sha256": "^0.2.0",
    "@types/uuid": "^8.3.0",
    "agenda": "^3.1.0",
    "axios": "^0.21.1",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "casbin": "^5.1.4",
    "client-sessions": "^0.8.0",
    "ejs": "^3.1.5",
    "events": "^3.2.0",
    "express": "^4.17.1",
    "firebase-admin": "^9.4.1",
    "http": "0.0.1-security",
    "jsonwebtoken": "^8.5.1",
    "loopback": "^3.27.0",
    "loopback-connector-kv-redis": "^3.0.3",
    "loopback-connector-mongodb": "^5.4.0",
    "moment": "^2.29.1",
    "multer": "^1.4.2",
    "net": "^1.0.2",
    "ngrok": "^3.3.0",
    "nodemailer": "^6.4.11",
    "nodemon": "^2.0.4",
    "passport": "^0.4.1",
    "passport-facebook": "^3.0.0",
    "passport-local": "^1.0.0",
    "path": "^0.12.7",
    "qs": "^6.9.4",
    "sha256": "^0.2.0",
    "tslib": "^2.0.1",
    "uuid": "^8.3.0"
  },
  "devDependencies": {
    "@loopback/build": "^6.2.2",
    "@loopback/testlab": "^3.2.4",
    "@types/node": "^10.17.32",
    "source-map-support": "^0.5.19",
    "typescript": "~3.9.7"
  },
  "nodemonConfig": {
    "verbose": true,
    "watch": [
      "src/"
    ],
    "ignore": [
      "dist/*"
    ],
    "ext": "ts",
    "exec": "npm start"
  }
}
